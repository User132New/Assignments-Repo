     "output_type": "stream",
     "text": [
      "Please enter the number whose table is required :9\n",
      "9 x 1 = 9\n",
      "9 x 2 = 18\n",
      "9 x 3 = 27\n",
      "9 x 4 = 36\n",
      "9 x 5 = 45\n",
      "9 x 6 = 54\n",
      "9 x 7 = 63\n",
      "9 x 8 = 72\n",
      "9 x 9 = 81\n",
      "9 x 10 = 90\n"
     ]
    }
   ],
   "source": [
    "a=int(input('Please enter the number whose table is required :'))\n",
    "e=1\n",
    "while (1<=e<=10):\n",
    "    d=a*e\n",
    "    print(f'{a} x {e} = {d}')\n",
    "    e+=1"
   ]
  }
