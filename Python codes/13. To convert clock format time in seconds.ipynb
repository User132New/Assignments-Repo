   "source": [
    "# To convert clock format time in seconds\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "33fb25cc",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Please enter clock time : 02:02:30\n",
      "The time in seconds is : 7350\n"
     ]
    }
   ],
   "source": [
    "t=input('Please enter clock time : ')\n",
    "h,m,s=t.split(':')\n",
    "h1, m1, s1 = map(int, (h, m, s))\n",
    "s2=h1*3600\n",
    "s3=m1*60\n",
    "sec=s1+s2+s3\n",
    "print(f'The time in seconds is : {sec}')"
   ]
  }
 ],
